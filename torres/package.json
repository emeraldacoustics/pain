  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "testbuild": "node node_modules/jest/bin/jest.js --coverage --forceExit --no-cache --passWithNoTests",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-direct-mutation-state": 0,
      "no-unused-vars": 0,
      "react/no-direct-mutation-state": 0
    }
  },
  "browser": {
    "crypto": false
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coveragePathIgnorePatterns": [
      "/public/",
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/public/",
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/public/",
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPathIgnorePatterns": [
      "/public/",
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.js",
      "\\.(scss|css|less)$": "<rootDir>/scripts/fileMock.js"
    },
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "./jest.polyfills.js",
      "react-app-polyfill/jsdom"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "process": "^0.11.10",
